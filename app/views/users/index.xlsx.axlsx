wb = xlsx_package.workbook
wb.add_worksheet(name: "Registrations") do |sheet|
  sheet.add_row ["University", "Delegates", "Name", "Email", "Phone", "Address"] ## Header values of Excel
  @registrations.each do |reg|
    sheet.add_row [reg.university, reg.delegates_count, reg.name, reg.email, reg.phone, reg.address]
  end
end
