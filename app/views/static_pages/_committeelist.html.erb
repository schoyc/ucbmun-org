<div class="row">
  <div class="committee">
    <% committees.each do |key, committee| %>
     <div class="col-md-4">
       <a href=<%= "committees/#{key}" %>>
       <div class="full-committee-profile" data-toggle="modal" data-target=<%=".#{key}"%>>
         <div class="committee-pic">
           <%= image_tag "committees/#{key}.jpg", class: "committee-picture" %>
         </div>
         <div class="committee-name">
           <h5><%= committee[:name] %></h5>
         </div>
       </div>
       </a>
     </div>
     <% end %>
</div>
</div>
