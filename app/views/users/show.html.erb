<%= render partial: "toolbar" %>
<div class="row">
<div class="profile-left col-md-8">
  <div class="user-profile-welcome">
    <h2> Welcome, </h2>
    <h2> <%= @user.university %> !</h2>
  </div>

  <div class="user-profile-news">
  </div>
</div>

<div class="profile-right col-md-4">
  <div class="user-profile-sidebar">
    <div class="user-profile-pic-wrapper">
      <%= image_tag "no_photo_person.png", class: "user-profile-pic" %>
    </div>
    <div class="user-profile-info" >
      <h4><%= @user.name %> </h4>
      <h5> <%=@user.email %> </h5>
      <h5> <%= @user.university %> </h5>
      <h6> <%=@user.address %> </h6>
      <h6> <%="#{@user.city}, #{@user.state} #{@user.zip}"%></h6>
      <h6> <%=@user.country %> </h6>
    </div>
  </div>
</div>
</div>
